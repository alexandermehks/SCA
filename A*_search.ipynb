    "#Implemenration from https://www.redblobgames.com/pathfinding/a-star/implementation.html\n",
    "import collections\n",
    "import heapq\n",
    "\n",
    "class PriorityQueue:\n",
    "    def __init__(self):\n",
    "        self.elements = []\n",
    "    \n",
    "    def empty(self):\n",
    "        return not self.elements\n",
    "    \n",
    "    def put(self, item, priority):\n",
    "        heapq.heappush(self.elements, (priority, item))\n",
    "    \n",
    "    def get(self):\n",
    "        return heapq.heappop(self.elements)[1]\n",
    "    \n",
    "    \n",
    "#Find neighbor function which I modified to not search for neighbors that are not in the maze.\n",
    "def neighbors(x,y):\n",
    "    nb = list()\n",
    "    xm=[0,1,0,-1]\n",
    "    ym=[1,0,-1,0]\n",
    "    for i in range(4):\n",
    "        #Added if to check so we are not outside of the maze\n",
    "        #Made it dynamicly count the max widht and height\n",
    "        #based on the maze w/h.\n",
    "        if 0 <= x+xm[i] < width*2 and 0 <= y+ym[i] < height*2:\n",
    "            #made the neighbors append as floats instead of arrays.\n",
    "            nb.append((x+xm[i],y+ym[i]))\n",
    "    return nb\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def weights(from_node, to_node):\n",
    "    weights = {}\n",
    "    return weights.get(to_node,1)\n",
    "\n",
    "\n",
    "    \n",
    "    \n",
    "#Counting on the heuristic or the H value\n",
    "#Its the estimated cost from current node to the end node\n",
    "#what the abs is doing is that it will return the absolute value without the negative sign -.\n",
    "def heuristic(a,b):\n",
    "    (x1, y1) = a\n",
    "    (x2, y2) = b\n",
    "    return abs(x1-x1) + abs (y1-y2)\n",
    "\n",
    "\n",
    "#A_star function to performa the A-star search algorithm\n",
    "def A_star(maze,start,end):\n",
    "    #Store activated nodes\n",
    "    \n",
    "    #Putting not activated queues in queue\n",
    "    frontier = PriorityQueue()\n",
    "    frontier.put(start,0)\n",
    "\n",
    "    #What node did we come from?\n",
    "    came_from = {}\n",
    "    came_from[start] = None\n",
    "    \n",
    "    #What is the cost so far?\n",
    "    cost_so_far = {}\n",
    "    cost_so_far[start] = 0\n",
    "\n",
    "\n",
    "    #While loop that will continue to iterate until there are no nodes left in the queue.\n",
    "    while not frontier.empty():\n",
    "        #Setting the current variable to the first node in the queue.\n",
    "        current = frontier.get()\n",
    "        #Making sure we are not using an invalid node(walls) in the calculation.\n",
    "        if maze[current] == 1:\n",
    "            continue\n",
    "        #If we have reached our goal node, we want the loop to break.\n",
    "        if current == end:\n",
    "            break\n",
    "        \n",
    "        #Calling on the neighbors function with the argument (x,y) for the current node.\n",
    "        #The function will return a float list with the neighbors to the current node.\n",
    "        neigh = neighbors(current[0],current[1])\n",
    "        #Iterating over the neighbors to the current node.\n",
    "        for n in neigh:\n",
    "            if n == 1:\n",
    "                print(\"test123\")\n",
    "            #Adding cost for the movement, based on the cost so far on the current node +\n",
    "            #the weights from the current node to the neighbor node.\n",
    "            new_cost = cost_so_far[current] + weights(current, n)\n",
    "            #Here im checking if the current iteration of the neighbor node is not in \n",
    "            #cost_so_far dict if thats the case im adding the neighbor to the dict with the value of new_cost.\n",
    "            #then im calculcating the priority based on the heuristic what will be the most optimal move.\n",
    "            #and then adding the node to the queue together with the priority of it\n",
    "            #but also adding the iterated node in the came_from dict together with the value\n",
    "            #of the current node.\n",
    "            if n not in cost_so_far or new_cost < cost_so_far[n]:\n",
    "                cost_so_far[n] = new_cost\n",
    "                priority = new_cost + heuristic(n,end)\n",
    "                frontier.put(n,priority)\n",
    "                came_from[n] = current\n",
    "                #print(f\"Adding {current} to came_from \\n\")        \n",
    "    #and lastly when there is no nodes left in the queue, im returning the two dictonaries, came_from\n",
    "    #                                                                                       cost_so_far\n",
    "    return came_from, cost_so_far\n",
    "\n",
    "\n",
    "\n",
    "#This is the function that will show the path.\n",
    "def path(maze,came_from, start, end):\n",
    "    current = end\n",
    "    path = []\n",
    "    #Loop that is making sure that we havent reached our goal yet. \n",
    "    #For each iteration we are appending the current node the the path array.\n",
    "    while current != start:\n",
    "        path.append(current)\n",
    "        current = came_from[current]\n",
    "    path.append(start)\n",
    "    #Reversing the path to get the optimal route from start to end. \n",
    "    path.reverse()\n",
    "    \n",
    "    #I've added a little bit of spice here where I iterate over the path array and for each node/element\n",
    "    #in the array and changing the maze value to 2. By doing this I can very easily see how the algoritm has\n",
    "    #moved. But I can also print out the data as an image with the Imshow method from matplotlib.\n",
    "    for p in path:\n",
    "        maze[p] = 2\n",
    "    return path, maze\n",
    "    \n",
    "\n",
    "\n",
    "\n",
    "start,goal = (1,1), (((width*2)-1),((height*2)-1))\n",
    "\n",
    "came_from, cost_so_far = A_star(maze,start,goal)\n",
    "\n",
    "print(f\" Original Maze \\n {maze2}\")\n",
    "\n",
    "\n",
    "p,m = path(maze,came_from, start,goal)\n",
    "\n",
    "print(f\"\\nSteps Taken\", p)\n",
    "print(f\"\\n Steps Taken set to 2 \\n\", m )\n",
    "\n",
    "imshow(maze)\n",
    "#print(f\"STEPS TAKEN\", p)\n",
    "\n",
    "#print(\"\\n\", maze)\n",
    "\n",
    "#djikstra(maze,(1,1),(10,11))\n",
    "\n",
    "\n",
    "#neighbors(1,1)"
   ]
